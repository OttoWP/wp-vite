{
  "name": "ottowp/wp-vite",
  "description": "A collection of back-end utilities for WordPress development with ViteJS.",
  "version": "0.1.0",
  "license": "BSD-2-Clause",
  "autoload": {
    "psr-4": {
      "WPVite\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": ["src/"]
  },
  "scripts": {
    "test": "vendor/bin/phpunit tests/unit",
    "analyze": "@php ./vendor/bin/phpstan analyze"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "brain/monkey": "^2.6",
    "szepeviktor/phpstan-wordpress": "^1.3.5",
    "php-stubs/wordpress-stubs": "^6.6"
  }
}
